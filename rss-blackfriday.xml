---
layout: null
navi:
 exclude: yes
---
<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:content="http://purl.org/rss/1.0/modules/content/"
xmlns:wfw="http://wellformedweb.org/CommentAPI/"
xmlns:dc="http://purl.org/dc/elements/1.1/"
version="2.0"
xmlns:atom="http://www.w3.org/2005/Atom"
xmlns:slash="http://purl.org/rss/1.0/modules/slash/"
xmlns:yandex="http://news.yandex.ru"
xmlns:media="http://search.yahoo.com/mrss/"
xmlns:turbo="http://turbo.yandex.ru">
  <channel>
    <title>{{ site.title | xml_escape }} Airdrops</title>
    <description>
      Crypto Black Friday 2020 - Live Deals
    </description>
    <image>
      <url>{{ site.url }}{{ site.baseurl }}/img/doge-share.png</url>
      <title>{{ site.title | xml_escape }}</title>
      <link>{{ site.url }}{{ site.baseurl }}</link>
    </image>
    <language>en</language>
    <link>{{ site.url }}{{ site.baseurl }}/blackfriday/</link>
    <atom:link href="{{ "/rss-blackfriday.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    {% assign live = site.ebf | where: "categories", "live" | reverse %}
    {% for post in live %}
      <item>
        <title><![CDATA[ {{ post.title | xml_escape }} ]]></title>
        <description><![CDATA[ {{ post.description | xml_escape }} ]]></description>
        <content:encoded><![CDATA[ {{ post.content | markdownify | strip_html | xml_escape }} ]]></content:encoded>
        <pubDate>{{ post.last_modified_at  | date_to_rfc822 }}</pubDate>
        <author>shill.shillerton@altcointrading.net</author>
        <webmaster>shill.shillerton@altcointrading.net</webmaster>
        {% if post.image[0] %}<media:content url="{{ post.image[0] | prepend: site.baseurl | prepend: site.url }}" medium="image" />{% endif %}
        <link>{{ "/blackfriday/#live-{{ post.tags[0] }}" | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ "/blackfriday/#live-{{ post.tags[0] }}" | prepend: site.baseurl | prepend: site.url }}</guid>
        {% for tag in post.tags %}<category><![CDATA[ {{ tag | xml_escape }} ]]></category>{% endfor %}
        {% for cat in post.categories %}<category><![CDATA[ {{ cat | xml_escape }} ]]></category>{% endfor %}
      </item>
    {% endfor %}
  </channel>
</rss>
