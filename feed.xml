---
layout: null
navi:
 exclude: yes
---
<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:content="http://purl.org/rss/1.0/modules/content/" xmlns:wfw="http://wellformedweb.org/CommentAPI/" xmlns:dc="http://purl.org/dc/elements/1.1/" version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:slash="http://purl.org/rss/1.0/modules/slash/">
  <channel>
    <title><![CDATA[  AltcoinTrading.NET - Articles ]]></title>
    <description><![CDATA[  {{ site.description | xml_escape }} ]]></description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <image>
      <url>{{ site.url }}{{ site.baseurl }}/img/logo.jpg</url>
      <title><![CDATA[  {{ site.title | xml_escape }} ]]></title>
      <link>{{ site.url }}{{ site.baseurl }}/</link>
    </image>
    <atom:link href="{{ "/feed.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% for post in site.posts limit:30 %}
      <item>
        <title><![CDATA[ {{ post.title | xml_escape }} ]]></title>
        <description><![CDATA[ {{ post.description | xml_escape }} ]]></description>
        <author><![CDATA[ {{ post.author_name }}@altcointrading.net ]]></author>
        <dc:creator><![CDATA[ {{ post.author_name }} ]]></dc:creator>

        <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
        <link>{{ post.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ post.url | prepend: site.baseurl | prepend: site.url }}</guid>
        <comments>{{ post.url | prepend: site.baseurl | prepend: site.url }}#comments</comments>
        <slash:comments>0</slash:comments>

        {% for tag in post.tags %}<category><![CDATA[ {{ tag | xml_escape }} ]]></category>
        {% endfor %}
        {% for cat in post.categories %}<category><![CDATA[ {{ cat | xml_escape }} ]]></category>
        {% endfor %}
        <content:encoded><![CDATA[ {{ post.description | xml_escape }} ]]></content:encoded>
      </item>
    {% endfor %}
  </channel>
</rss>
